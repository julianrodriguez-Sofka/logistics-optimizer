{
  "name": "logistics-back",
  "version": "1.0.0",
  "description": "Backend for Logistics Shipping Optimizer",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test:api": "newman run ../postman/postman_collection.json -e ../postman/postman_environment.json --reporters cli,html --reporter-html-export newman-report.html",
    "test:api:ci": "newman run ../postman/postman_collection.json -e ../postman/postman_environment.json --reporters cli,json --reporter-json-export test-results.json"
  },
  "keywords": [
    "logistics",
    "shipping",
    "tdd"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^9.1.1",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.2",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^9.5.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@types/amqplib": "^0.10.8",
    "@types/mongoose": "^5.11.97",
    "@types/socket.io": "^3.0.1",
    "amqplib": "^0.10.9",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "express": "^4.18.0",
    "express-rate-limit": "^8.2.1",
    "mongoose": "^8.21.0",
    "socket.io": "^4.8.3"
  }
}
